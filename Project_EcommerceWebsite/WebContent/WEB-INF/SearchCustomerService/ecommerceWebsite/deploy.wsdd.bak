<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SearchCustomerService WSDL service -->

  <service name="SearchCustomer" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ecommerceWebsite"/>
      <parameter name="wsdlServiceElement" value="SearchCustomerService"/>
      <parameter name="schemaQualified" value="http://ecommerceWebsite"/>
      <parameter name="wsdlServicePort" value="SearchCustomer"/>
      <parameter name="className" value="ecommerceWebsite.SearchCustomerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="SearchCustomer"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="searchCustomerInfo" qname="operNS:searchCustomerInfo" xmlns:operNS="http://ecommerceWebsite" returnQName="retNS:searchCustomerInfoReturn" xmlns:retNS="http://ecommerceWebsite" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:CustomerID" xmlns:pns="http://ecommerceWebsite" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="searchCustomerInfo"/>

  </service>
</deployment>
